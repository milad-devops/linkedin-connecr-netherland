{
  "name": "linkedin-auto-connect",
  "version": "1.0.0",
  "buildTag": "latest",
  "environmentVariables": {},
  "input": {
    "title": "LinkedIn Auto Connect",
    "type": "object",
    "schema": {
      "type": "object",
      "properties": {
        "LINKEDIN_EMAIL": {
          "type": "string",
          "title": "LinkedIn Email"
        },
        "LINKEDIN_PASSWORD": {
          "type": "string",
          "title": "LinkedIn Password"
        },
        "SEARCH_KEYWORD": {
          "type": "string",
          "title": "Search Keyword"
        },
        "CONNECT_MESSAGE": {
          "type": "string",
          "title": "Connect Message"
        },
        "dailyLimit": {
          "type": "number",
          "title": "Daily Limit"
        }
      },
      "required": [
        "LINKEDIN_EMAIL",
        "LINKEDIN_PASSWORD",
        "SEARCH_KEYWORD"
      ]
    }
  }
}
